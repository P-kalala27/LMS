{
    "version": 2,
    "build": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config" : {
                "includeFile" : [
                    "dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}